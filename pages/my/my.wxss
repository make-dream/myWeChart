page .section .work .group {
  height: 100rpx;
  border-bottom: 1px solid #e4e4e4;
  background: white;
  padding: 0 30rpx;
  overflow: hidden;
  line-height: 100rpx;
}
page .section .work image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 21rpx;
  vertical-align: middle;
}
page .section .work .key {
  color: #666;
  font-size: 29rpx;
}
page .section .work .box {
  width: 70%;
  float: right;
  color: #333;
  font-size: 29rpx;
  overflow: hidden;
  text-align: right;
}
page .section .work .name {
  text-align: right;
  color: #333;
  font-size: 29rpx;
  float: right;
}
page .section .work .active {
  display: block;
  position: relative;
  width: 1000rpx;
}
page .section .logout {
  position: fixed;
  bottom: 120rpx;
  left: 5%;
  background: #999999;
  margin: 0 auto;
  font-size: 28rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  border-radius: 2px;
  width: 90%;
  color: white;
}
