page {
  height: 100%;
  background: #f5f5f5;
  position: relative;
}
page .section {
  padding-top: 21rpx;
}
page .section .user {
  height: 146rpx;
  border-top: 1px solid #e4e4e4;
  border-bottom: 1px solid #e4e4e4;
  background: white;
  margin-bottom: 31rpx;
}
page .section .user image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 21rpx 31rpx 0 33.3rpx;
  float: left;
  vertical-align: middle;
}
page .section .user text {
  float: left;
  line-height: 140rpx;
  color: #333333;
  font-size: 38rpx;
}
