/* pages/list/list.wxss */
page {
  height: 100%;
  background: #f5f5f5;
}
.section {
  padding: 25rpx 0 25rpx 33.3rpx;
  background: #0898ff;
}
.section picker {
  width: 246rpx;
  height: 71rpx;
  color: #0898ff;
  display: inline-block;
  background: white;
}
.section .find {
  color: white;
  font-size: 29rpx;
  line-height: 71rpx;
  margin-right: 33.3rpx;
  float: right !important;
}
.section .picker {
  color: #0898ff;
  font-size: 29rpx;
  display: inline-block;
  line-height: 71rpx;
}
.section .picker image {
  width: 28rpx;
  height: 28rpx;
  margin: 0 21rpx;
  vertical-align: middle;
}
.section button {
  display: inline-block;
  width: 129rpx;
  background: white;
  height: 71rpx;
  line-height: 71rpx;
  color: #0898ff;
  font-size: 27rpx;
  padding: 0 !important;
}
.section button.prev {
  margin-right: 2rpx;
  border-radius: 4rpx 0 0 4rpx;
}
.section button.next {
  margin-left: 2rpx;
  border-radius: 0 4rpx 4rpx 0;
}
.section .pull-left {
  float: left;
}
.content .main {
  background: #f5f5f5;
}
.content .main .order {
  box-sizing: border-box;
  margin-top: 21rpx;
  margin-bottom: 21rpx;
}
.content .main .order text {
  width: 50%;
  display: inline-block;
  font-size: 25rpx;
  color: #999;
  text-align: center;
}
.content .main .order text.active {
  color: #0898ff;
}
.content .main .order.pay {
  border-right: 1rpx solid #e4e4e4;
}
.content .list {
  background: #FFFFFF;
  border-bottom: 2px solid #F2F2F2;
  height: 133rpx;
  display: flex;
  align-items: center;
  position: relative;
  z-index: 1;
}
.content .list view {
  float: left;
  font-size: 14px;
  color: #333;
}
.content .list view text {
  display: block;
}
.content .list view image {
  width: 83rpx;
  height: 83rpx;
}
.content .list view.number {
  flex: 1;
  color: #999;
  font-size: 25rpx;
}
.content .list view.number .phone {
  color: #333;
  padding-bottom: 10rpx;
  font-size: 33.3rpx;
}
.content .list view.type {
  text-align: center;
  width: 83rpx;
  height: 83rpx;
  margin-left: 36rpx;
  margin-right: 25rpx;
}
.content .list view.price {
  flex: 1;
  text-align: right;
  margin-right: 33.3rpx;
  color: #999;
  font-size: 25rpx;
}
.content .list view.price .status {
  color: #333;
  padding-bottom: 10rpx;
  font-size: 33.3rpx;
}
.content .list view.price .status.active {
  color: #f94444;
}
.content .error {
  padding-top: 41rpx;
  color: #666;
  font-size: 30rpx;
  display: block;
  text-align: center;
}
.content .nomore {
  color: #666;
  font-size: 36rpx;
  text-align: center;
  padding: 41rpx 0;
}
.loading {
  padding-bottom: 200rpx;
}
.weui-loading {
  margin: 0 5rpx;
  width: 41rpx;
  height: 41rpx;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5rpx auto;
  line-height: 1.6rpx;
  font-size: 25rpx;
  text-align: center;
  color: #666;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
