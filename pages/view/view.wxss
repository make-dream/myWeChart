page {
  height: 100%;
  background: #f5f5f5;
  color: #666;
}
.team {
  background: white;
}
.team .group {
  font-size: 29rpx;
  padding: 0 33rpx 33rpx 33rpx;
}
.team .group .value {
  color: #999;
  float: right;
}
.team .group .name {
  color: #333;
}
.team .group.boss {
  padding-bottom: 42rpx;
}
.team.content {
  padding-top: 42rpx;
  border-top: 1rpx solid #F2F2F2;
  border-bottom: 1rpx solid #F2F2F2;
}
.user {
  background: white;
  padding-top: 67rpx;
}
.user .time {
  padding-top: 42rpx;
  border-top: 1rpx solid #F2F2F2;
  border-bottom: 1rpx solid #F2F2F2;
  margin-bottom: 21rpx;
}
.user .price {
  font-size: 62rpx;
  color: #333;
  text-align: center;
  margin-bottom: 33rpx;
}
.user .status {
  font-size: 29rpx;
  text-align: center;
  margin-bottom: 83rpx;
}
.clearfix {
  _zoom: 1;
}
.clearfix:before,
.clearfix:after {
  display: table;
  line-height: 0;
  content: "";
}
.clearfix:after {
  clear: both;
}
