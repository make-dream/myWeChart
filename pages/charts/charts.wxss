page {
  height: 100%;
  background: white;
}
page .chart {
  width: 100%;
}
page .chart .tablist {
  width: 684rpx;
  margin: 25rpx auto;
  font-size: 27rpx;
  color: #0898ff;
}
page .chart .tablist text {
  text-align: center;
  float: left;
  width: 50%;
  height: 71rpx;
  line-height: 71rpx;
  border: 2rpx solid #0898ff;
  box-sizing: border-box;
}
page .chart .tablist text.noborder {
  border-right: none;
}
page .chart .tablist text.active {
  background: #0898ff;
  color: white;
  border: none;
}
page .chart .orderlist {
  background: #f5f5f5;
  color: #666;
}
page .chart .orderlist .team {
  margin: 21rpx 0;
  padding-left: 46rpx;
  font-size: 28rpx;
  float: left;
  width: 50%;
  box-sizing: border-box;
}
page .chart .orderlist .team text {
  display: inline-block;
}
page .chart .orderlist .team text.name {
  font-size: 25rpx;
  color: #999;
  margin-bottom: 21rpx;
}
page .chart .orderlist .team text.val {
  font-size: 33.3rpx;
  color: #333;
  width: 100%;
}
page .chart .orderlist .team text.red {
  color: #f94444;
  font-size: 29rpx;
  float: right;
  margin-right: 46rpx;
}
page .chart .orderlist .team text.green {
  color: #15c58b;
  font-size: 29rpx;
  float: right;
  margin-right: 46rpx;
}
page .chart .orderlist .team.border {
  border-right: 2rpx solid #ececee;
}
page .chart .con {
  background: white;
}
page .chart .con .title {
  text-align: center;
  font-size: 33.3rpx;
  color: #333;
  margin-bottom: 10rpx;
}
page .chart .con .tit {
  text-align: center;
  font-size: 33.3rpx;
  color: #333;
  margin-bottom: 10rpx;
  margin-top: 54rpx;
}
page .chart .con .type {
  font-size: 25rpx;
  color: #999;
  padding-right: 46rpx;
  display: block;
  text-align: right;
  margin-bottom: 20rpx;
}
page .chart .con .canvas {
  padding-bottom: 30rpx;
}
page .chart .content {
  background: #f5f5f5;
  padding-top: 20rpx;
  padding-bottom: 120rpx;
}
page .chart .content .title {
  background: white;
  color: #333;
  padding-top: 52rpx;
  padding-bottom: 46rpx;
  font-size: 33.3rpx;
  text-align: center;
  display: block;
}
page .chart .content .main {
  z-index: 2;
  position: relative;
  height: 100%;
}
page .chart .content.active {
  padding: 0;
}
page .chart canvas {
  display: block;
}
page .chart .group {
  background: white;
  font-size: 24rpx;
  color: #333333;
}
page .chart .group .block {
  width: 684rpx;
  height: 71rpx;
  margin: 0 auto;
  margin-bottom: 33.3rpx;
  position: relative;
}
page .chart .group .main view {
  float: left;
}
page .chart .group .main view.progress {
  float: none;
}
page .chart .group .main view.progress .pro {
  float: left;
  height: 8.33rpx;
  width: 65%;
  background: #e9f6ff;
  border-radius: 4rpx;
}
page .chart .group .main view.progress .pro .active {
  line-height: normal;
  display: inline-block;
  background: #0a97ff;
  border-radius: 4rpx;
  height: 100%;
  float: left;
}
page .chart .group .main view.progress .num {
  color: #0898ff;
  font-size: 21rpx;
  float: left;
  line-height: 8rpx;
}
page .chart .group .color {
  height: 50rpx;
  position: absolute;
}
page .chart .group .no {
  background: #6dc3fe;
  border-radius: 50%;
  display: inline-block;
  width: 71rpx;
  height: 71rpx;
  color: white;
  text-align: center;
  line-height: 71rpx;
  margin-right: 31rpx;
  font-size: 42rpx;
}
page .chart .group .no.glod {
  background: #FEAA0C;
}
page .chart .group .name {
  width: 65%;
  color: #333;
  font-size: 29rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  height: 100%;
}
page .chart .group .name .location {
  width: 300%;
  line-height: 55rpx;
  height: 50rpx;
  color: #333;
  font-size: 29rpx;
  text-align: left;
  display: inline-block;
  position: relative;
}
page .chart .group .num2 {
  color: #999;
  float: right!important;
  font-size: 29rpx;
  line-height: 71rpx;
}
